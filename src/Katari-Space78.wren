var WIDTH=240
var HEIGHT=136
 
var BTN_UP=0
var BTN_DOWN=1
var BTN_LEFT=2
var BTN_RIGHT=3
var BTN_A=4
var BTN_B=5
var BTN_X=6
var BTN_Y=7
var KEY_SPC=48

class Entity {

  construct new(x, y) {
    _x = x
    _y = y
  }
  
  x {_x}
  y {_y}
  
  x=(value) {_x = value}
  y=(value) {_y = value}
  
  update() {
  }
  
  draw() {
  }
}

class Collision {

  static check(a, b) {

    var xd = ((a.x + (a.w/2)) - (b.x + (b.w/2))).abs
    var xs = a.w*0.5 + b.w*0.5
    var yd = ((a.y + (a.h/2)) - (b.y + (b.h/2))).abs
    var ys = a.h/2 + b.h / 2
    
    return (xd < xs && yd < ys)
  }
  
  static debug(a, b) {
  
    var xd = ((a.x + (a.w/2)) - (b.x + (b.w/2))).abs
    var xs = a.w/2 + b.w/2
    var yd = ((a.y + (a.h/2)) - (b.y + (b.h/2))).abs
    var ys = a.h/2 + b.h/2  
    
    TIC.print("xd: %(xd)", WIDTH - 6-40, 4)
    TIC.print("xs: %(xs)", WIDTH - 6-40, 10)
    TIC.print("yd: %(yd)", WIDTH - 6-40, 16)
    TIC.print("ys: %(ys)", WIDTH - 6-40, 22)

    TIC.trace("xd: %(xd)")
    TIC.trace("xs: %(xs)")
    TIC.trace("yd: %(yd)")
    TIC.trace("ys: %(ys)")
  }
}

class Hitbox is Entity {

  construct new(entity) {
    super(x, y)
    _w = entity.w
    _h = entity.h

    _entity = entity
  }

  update() {
     x = _entity.x
     y = _entity.y
    _w = _entity.w
    _h = _entity.h
  }
}

var BULLETS = []

class Bullet is Entity {

  construct new(x, y, speed) {
    super(x, y)

    _w = 4
    _h = 1
    _speed = speed

    _hitbox = Hitbox.new(this)
  }

  w {_w}
  h {_h}

  hitbox {_hitbox}
  
  update() {
    if (x > WIDTH) {
      BULLETS.remove(this)
      return
    }
    
    _hitbox.update()
    x = x + _speed
  }
  
  draw() {
    TIC.rect(x, y, _w, _h, 0)
  }
}

class Player is Entity {

  construct new(x, y) {
    super(x, y)
    _w = 2
    _h = 2

    _hitbox = Hitbox.new(this)
  }

  w {_w}
  h {_h}

  hitbox {_hitbox}
  
  fire() {
    TIC.spr(2, x+1, y-4)
    BULLETS.add(Bullet.new(x+5, y+1, 6))
    TIC.sfx(0, "A-4", 6)
  }
  
  update() {
    if (TIC.btn(BTN_UP)) {
      y = y - 1
    }
    if (TIC.btn(BTN_DOWN)) {
      y = y + 1
    }
    
    if (TIC.btnp(BTN_A) || TIC.keyp(KEY_SPC, 1, -1)) {
      this.fire()
    }

    _hitbox.update()
  }
  
  draw() {
    TIC.rect(x, y, _w, _h, 0)
  }
}

var ENEMIES = []

class Enemy is Entity {


  construct new(x, y, seed) {
    super(x, y)

    import "random" for Random
    
    _w = 4
    _h = 4
    
    _random = Random.new(seed)
    _speed = _random.int(1, 3.5)

    _highBoundary = 20
    _lowBoundary = HEIGHT - 20

    _hitbox = Hitbox.new(this)
  }

  w {_w}
  h {_h}

  hitbox {_hitbox}
  
  checkHit() {
    BULLETS.each {|bullet|
      if (Collision.check(this, bullet)) {
        TIC.sfx(1, "A-4", 4)
        TIC.spr(3, x, y)
        BULLETS.remove(bullet)
        ENEMIES.remove(this)
      }
      if (ENEMIES.count == 0) {
        TIC.music(0, -1, -1, true)
      }
    }
  }

  update() {
    var isHigher = y < _highBoundary
    var isLower = y > _lowBoundary
    
    if (isHigher || isLower) {
      _speed = -_speed
    }

    y = y + _speed

    _hitbox.update()
    checkHit()
  }
  
  draw() {
   TIC.rect(x, y, _w, _h, 0)
  }
}

class Game is TIC{

	construct new(){
		_tick=0
		
		_p1 = Player.new(4, 50)
		
		_e1 = Enemy.new(WIDTH - 6, 50, 48)
		_e2 = Enemy.new(WIDTH - 12, 50, 36)
		_e3 = Enemy.new(WIDTH - 18, 50, 8)
		_e4 = Enemy.new(WIDTH - 24, 50, 18)
		
		_e5 = Enemy.new(WIDTH - 30, 50, 48)
		_e6 = Enemy.new(WIDTH - 42, 50, 36)
		_e7 = Enemy.new(WIDTH - 60, 50, 8)
		_e8 = Enemy.new(WIDTH - 84, 50, 18)
		
		ENEMIES = [_e1, _e2, _e3, _e4, _e5, _e6, _e7, _e8]
	}
	
	UPDATE() {
	  _p1.update()
    
    ENEMIES.each {|enemy|
      enemy.update()
    }
    
    BULLETS.each {|bullet|
      bullet.update()
    }
    
    if (ENEMIES.count == 0) {
  		TIC.print("YOU WON!", WIDTH/2, HEIGHT/2, 5)
    }
	}
	
	DRAW() {
    _p1.draw()

    ENEMIES.each {|enemy|
      enemy.draw()
    }
    
    BULLETS.each {|bullet|
      bullet.draw()
    }
  }
	
	TIC(){
		TIC.cls(13)
		this.UPDATE()
		this.DRAW()
		
		TIC.print("BULLETS: %(BULLETS.count)", 4, 1)
		TIC.print("ENEMIES: %(ENEMIES.count)", 4, 8)
  }
}

